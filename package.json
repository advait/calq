{
  "name": "calq",
  "version": "0.0.3",
  "repository": "git@github.com:advait/calq.git",
  "author": "Advait Shinde <advait.shinde@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:spago": "spago build --watch",
    "test": "yarn build:spago && node --loader './engine/test/custom-loader.mjs' ./engine/test/test-runner.mjs",
    "build": "yarn run build:spago && vite build",
    "build:spago": "spago build",
    "serve": "serve dist -l 8080",
    "clean": "rm -rf output dist",
    "lint:purs": "purs-tidy format-in-place 'engine/**/*.purs'",
    "lint:purs:ci": "purs-tidy check 'engine/**/*.purs'",
    "lint:ts": "eslint frontend && prettier frontend --write",
    "lint:ts:ci": "eslint frontend && prettier frontend",
    "lint": "yarn run lint:purs && yarn run lint:ts",
    "lint:ci": "yarn run lint:purs:ci && yarn run lint:ts:ci",
    "ci": "yarn run build && yarn run test && yarn run lint:ci",
    "gh-pages": "yarn run clean && yarn run build && echo 'calq.app' > dist/CNAME && gh-pages -d dist/"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@chakra-ui/system": "^2.6.1",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/roboto-mono": "^5.0.9",
    "@fontsource/roboto": "^5.0.8",
    "decimal.js": "^10.3.1",
    "framer-motion": "^10.16.4",
    "qs": "^6.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.49.0",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "purescript": "^0.15.10",
    "purescript-psa": "^0.8.2",
    "purs-tidy": "^0.10.0",
    "purty": "^7.0.0",
    "serve": "^14.2.1",
    "spago": "0.21.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-ejs": "^1.6.4"
  }
}
