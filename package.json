{
  "name": "calq",
  "version": "0.0.2",
  "browser": true,
  "repository": "git@github.com:advait/calq.git",
  "author": "Advait Shinde <advait.shinde@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/spago build",
    "test": "./node_modules/.bin/spago test",
    "serve": "./node_modules/.bin/parcel dev/index.html --open",
    "build-prod": "rm -rf output dist && yarn run build && ./node_modules/.bin/parcel build dev/index.html",
    "gh-pages": "yarn run build-prod && echo 'calq.app' > dist/CNAME && ./node_modules/.bin/gh-pages -d dist/"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.5",
    "decimal.js": "^10.3.1",
    "react": ">=16.3",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.0.0-rc.0",
    "@parcel/transformer-image": "2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "@parcel/transformer-webmanifest": "^2.0.0-rc.0",
    "gh-pages": "^3.1.0",
    "parcel": "^2.0.0-rc.0",
    "purescript": "^0.14.5",
    "purescript-psa": "^0.8.0",
    "purty": "^7.0.0",
    "sass": "^1.30.0",
    "spago": "^0.17.0"
  },
  "alias": {
    "fs": false
  }
}
